def print_full_name():
    first_name= input()
    Trialattempts=[1,2,3]
    for i in Trialattempts:
        if len(first_name) <=10:
            first_name=first_name
        else:
            print("Your characters exceed maximum")
            return

    last_name=input()
    Trialattempts=[1,2,3]
    for i in Trialattempts:
        if len(last_name)<=10:
            last_name=last_name
        else:
            print("Your characters exceed maximum")
            return

    print(f' Hello {first_name} {last_name}. You delved into python!')
